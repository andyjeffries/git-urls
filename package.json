{
  "name": "git-urls",
  "version": "0.3.0",
  "description": "Get online URL of file(Github, GitLab, Bitbucket...)",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qinezh/git-urls.git"
  },
  "keywords": [
    "git"
  ],
  "author": "qinezh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qinezh/git-urls/issues"
  },
  "homepage": "https://github.com/qinezh/git-urls#readme",
  "contributors": [
    "Junle Li",
    "Scott Bonebrake"
  ],
  "dependencies": {
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.22",
    "ci-publish": "^1.3.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest",
    "ci-publish": "ci-publish"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
